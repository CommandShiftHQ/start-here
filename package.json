{
  "name": "start-here",
  "version": "1.0.0",
  "description": "mcr.codes start here repo",
  "main": "main/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "prepush": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MCRcodes/start-here.git"
  },
  "author": "Susana Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MCRcodes/start-here/issues"
  },
  "homepage": "https://github.com/MCRcodes/start-here#readme",
  "devDependencies": {
    "jest": "^24.8.0",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {},
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
